    "@neondatabase/toolkit": "^0.1.2",
    "@netlify/neon": "^0.1.2",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.563.0",
    "marked": "^17.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@netlify/functions": "^5.1.2",
