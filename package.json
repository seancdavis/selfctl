{
  "name": "personal-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && node scripts/generate-redirects.js",
    "generate-redirects": "node scripts/generate-redirects.js",
    "lint": "eslint .",
    "preview": "vite preview",
